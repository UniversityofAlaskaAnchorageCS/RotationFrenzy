@page
@model FeedbackModel
@{
    ViewData["Title"] = "Feedback";
}
<h2>@ViewData["Title"]</h2>

<form>
    <p>Have feedback or questions? Let us know!</p>
    <div class="form-group">
        <label for="name-input">Name:</label>
        <input class="form-control" id="name-input" type="text" />
    </div>
    <div class="form-group">
        <label for="feedback">Feedback:</label>
        <textarea class="form-control" id="feedback" rows="5"></textarea>
    </div>
    <div class="form-group">
        <label for="contact-info">Contact Information (Optional):</label>
        <input class="form-control" id="contact-info" type="text" />
    </div>
    <button type="button" class="btn btn-primary">Submit</button>
</form>
