<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.badlogic.gdx.physics.box2d.box2d-gwt' />

	<inherits name='RotationFrenzy' />
	<entry-point class='com.uaa.rotationfrenzy.client.HtmlLauncher' />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	<set-configuration-property name="gdx.assetpath" value="../android/assets" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.level.AssetProperties" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.level.Level" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.entity.Den" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.entity.Acorn" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.entity.Eagle" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.entity.Squirrel" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.entity.Wheel" />
	<extend-configuration-property name="gdx.reflect.include" value="com.uaa.rotationfrenzy.graph.BasicGraph" />
	<extend-configuration-property name="gdx.reflect.include" value="java.util.ArrayList" />
</module>
